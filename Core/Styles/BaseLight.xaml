<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Color x:Key="BlackColor">#FF000000</Color>
    <Color x:Key="WhiteColor">#FFFFFFFF</Color>

    <Color x:Key="NormalForegroundColor">#FF000000</Color>
    <Color x:Key="HoverForegroundColor">#FFFFFFFF</Color>
    
    <Color x:Key="Gray5">#FFe8e8ec</Color>
    <Color x:Key="Gray6">#FFFFFFFF</Color>
    <Color x:Key="Gray7">#FFf6f6f6</Color>
    <Color x:Key="Gray8">#FFefeff2</Color>

    <Color x:Key="Gray19">#FFcccedb</Color>
    <Color x:Key="Gray20">#FFe7e8ec</Color>

    <Color x:Key="GrayNormal">#FFBEBEBE</Color>
    <Color x:Key="GrayHover">#FF333333</Color>

    <SolidColorBrush x:Key="BlueBrush" Color="#FFaaaad0"/>
    <SolidColorBrush x:Key="YellowBrush" Color="#FFe7dda3"/>
    <SolidColorBrush x:Key="GreenBrush" Color="#FF9ad09a"/>
    <SolidColorBrush x:Key="RedBrush" Color="#FFe09a9a"/>
</ResourceDictionary>